{
  "name": "showcase",
  "version": "0.0.1",
  "engines": {
    "node": "^18.20.4"
  },
  "private": true,
  "type": "module",
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "sst:dev": "sst dev 'pnpm run dev'",
    "sst:diff": "sst diff",
    "deploy:dev": "sst deploy",
    "deploy:prod": "sst deploy --stage prod",
    "deploy:remove": "sst remove",
    "sst:console": "sst console",
    "typecheck": "tsc --noEmit",
    "test": "sst bind -- vitest run",
    "prebuild": "rm -rf ./.build/ && mkdir ./.build && cp ./adapters/aws-lambda/index.js ./.build/index.js && cp ./adapters/aws-lambda/function.package.json ./.build/package.json",
    "build": "sst shell -- 'qwik build'",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/aws-lambda/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "node": "node",
    "preview": "qwik build preview && vite preview --open",
    "qwik": "./node_modules/qwik/bin/qwik"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.0.20",
    "@fontsource/ibm-plex-mono": "^5.0.13",
    "@supabase/supabase-js": "^2.44.4",
    "fontaine": "^0.5.0",
    "marked": "^13.0.3",
    "notion-to-md": "^3.1.1",
    "shiki": "^1.13.0",
    "sst": "3.0.83"
  },
  "devDependencies": {
    "aws-cdk-lib": "2.142.1",
    "constructs": "10.3.0",
    "typescript": "^5.5.4",
    "@tsconfig/node18": "^18.2.4",
    "@builder.io/qwik": "^1.8.0",
    "@builder.io/qwik-city": "^1.8.0",
    "@notionhq/client": "^2.2.15",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.56.10",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "compression": "^1.7.4",
    "dotenv": "^16.3.2",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.5.5",
    "prettier": "^3.2.5",
    "serverless-http": "^3.2.0",
    "source-map-support": "^0.5.21",
    "undici": "*",
    "vite": "^5.2.10",
    "vite-tsconfig-paths": "^4.2.1"
  }
}