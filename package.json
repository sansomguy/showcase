{
  "name": "showcase",
  "version": "0.0.1",
  "engines": {
    "node": "^18.20.4"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "sst:dev": "sst dev",
    "sst:diff": "sst diff",
    "deploy:dev": "sst deploy",
    "deploy:prod": "sst deploy --stage prod",
    "deploy:remove": "sst remove",
    "sst:console": "sst console",
