  },
  "devDependencies": {
    "sst": "^3.0.83",
    "aws-cdk-lib": "2.142.1",
    "constructs": "10.3.0",
    "typescript": "^5.5.4",
    "@tsconfig/node18": "^18.2.4"
  },
  "workspaces": [
    "packages/*"
  ]
}